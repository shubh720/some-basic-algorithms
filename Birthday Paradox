#include <iostream>
using namespace std;

int main() {
   
 int people = 0;  // no person   
 
 float psame;
 cin >> psame;
 
 float pnotsame = 1 - psame;
 float x = 1.0;
 float num = 365,den=365;

    while(x > pnotsame){
      x = (num/den)*x;     
      num--;
      people++;
    }

 cout << people <<endl;
}
